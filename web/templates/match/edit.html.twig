{% extends 'base.html.twig' %}

{% block title %}Set match result{% endblock %}

{% block body %}
    {{ render_flash_messages() }}

    <h1>Set match result</h1>

    {{ form_start(form) }}
        {{ form_row(form.start_time) }}
        {{ form_row(form.duration) }}
        {{ form_row(form.submit, {
            label: 'Submit'
        }) }}
    {{ form_end(form) }}

{% endblock %}
